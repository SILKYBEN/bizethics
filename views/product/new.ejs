						<form method="post" action="/product/create">
							<div class="form-group">
				              <label class="cols-sm-2 control-label"> Product Name </label>
				              <div class="cols-sm-10">
				                <div class="input-group">
				                  <span class="input-group-addon"><i class="fa fa-envelope fa" aria-hidden="true"></i></span>
				                  <input type="text" class="form-control" name="name" id="name"  placeholder="Enter Your Product Name"/>
				                </div>
				              </div>
				            </div>
				            <div class="form-group">
				              <label class="cols-sm-2 control-label"> Price of Product </label>
				              <div class="cols-sm-10">
				                <div class="input-group">
				                  <span class="input-group-addon"><i class="fa fa-envelope fa" aria-hidden="true"></i></span>
				                  <input type="number" class="form-control" name="amount" id="amount"  placeholder="Enter The Product Price"/>
				                </div>
				              </div>
				            </div>
				            <div class="form-group">
				              <label class="cols-sm-2 control-label"> Category Of Product </label>
				              <div class="cols-sm-10">
				                <div class="input-group">
				                  <span class="input-group-addon"><i class="fa fa-envelope fa" aria-hidden="true"></i></span>
				                  <select name="category" >
						                        <option value="">Select Product Category</option>
						                        <option value="cctv">CCTV</option>
						                        <option value="structuredCabling">Structured Cabling</option>
						                        <option value="fiberOptics">Fiber Optics</option>
						                        <option value="powerConditioners">Power Conditioners</option>
						                        <option value="accessControl">Access Control</option>
						                        <option value="others">Others</option>
						          </select>
				                </div>
				              </div>
				            </div>
				             <div class="form-group">
				              <label class="cols-sm-2 control-label"> Type Of Product </label>
				              <div class="cols-sm-10">
				                <div class="input-group">
				                  <span class="input-group-addon"><i class="fa fa-envelope fa" aria-hidden="true"></i></span>
				                  <select name="category" >
						                        <option value="">Select Product Type</option>
						                        <option value="switches">Switches/Routers</option>
						                        <option value="connectors">Connectors</option>
						                        <option value="tools">Testing,Structure Cabling, tools and acessories</option>
						                        <option value="converters">Converters</option>
						                        <option value="telephony">Telephony</option>
						                        <option value="security">Security&amp;Surveillance</option>
						                        <option value="lanCables">Lan Cables</option>
						                        <option value="fiberCables">Fiber Cables</option>
						                        <option value="ipCameras">IP Cameras</option>
						                        <option value="hdCameras">HD Cameras</option>
						                        <option value="ahdCameras">AHD Cameras</option>
						                        <option value="4in1Cameras">4 in 1 Cameras</option>
						                        <option value="nvr">NVR</option>
						                        <option value="dvr">DVR</option>
						                        <option value="others">Others</option>
						          </select>
				                </div>
				              </div>
				            </div>
				            <div class="form-group">
				              <label class="cols-sm-2 control-label"> Brand </label>
				              <div class="cols-sm-10">
				                <div class="input-group">
				                  <span class="input-group-addon"><i class="fa fa-envelope fa" aria-hidden="true"></i></span>
				                  <select name="category" >
						                        <option value="">Select Brand of Product</option>
						                        <option value="cisco">Cisco</option>
						                        <option value="ubiquiti">Ubiquiti</option>
						                        <option value="mikrotik">Mikrotik</option>
						                        <option value="engenius">Engenius</option>
						                        <option value="lsvision">LsVision</option>
						                        <option value="lsvision">Netgear</option>
						                        <option value="others">Others</option>
						          </select>
				                </div>
				              </div>
				            </div>
				            <div class="form-group">
				              <label for="occupation" class="cols-sm-2 control-label"> Description </label>
				              <div class="cols-sm-10">
				                <div class="input-group">
				                  <span class="input-group-addon"><i aria-hidden="true"></i></span>
				                  <textarea name="description" id="description" placeholder="Description" rows="5"></textarea>
				                </div>
				              </div>
				            </div>
							<button type="submit" class="btn btn-default">Next</button>
						</form>
